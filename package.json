{
  "name": "webpack-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev && npm run lint:watch",
    "dev": "cross-env NODE_ENV=mock ./node_modules/.bin/webpack-dev-server --config ./config/webpack.config.js --mode development",
    "pord": "cross-env NODE_ENV=pord ./node_modules/.bin/webpack-dev-server --config ./config/webpack.config.js --mode production",
    "build-dev": "cross-env NODE_ENV=mock ./node_modules/.bin/webpack --config ./config/webpack.config.js --progress --colors --mode development",
    "build-pord": "cross-env NODE_ENV=pord ./node_modules/.bin/webpack --config ./config/webpack.config.js --mode production",
    "test": "cross-env NODE_ENV=mock && node ./config/webpack.plugins",
    "lint": "esw ./config/*.config.* ./src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "lint:style": "stylelint --fix --syntax scss src/**/*.scss"
  },
  "entry": {
    "index": "./src/index.js",
    "test": "./src/test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "json-loader": "^0.5.7",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "my-ip": "^1.1.1",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
